{
  "plugins": [
    "prettier"
  ],
  "env": {
    "es6": true,
    "node": true,
    "browser": false
  },
  "parser": "@typescript-eslint/parser",
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "prettier/prettier": "warn",
    "@typescript-eslint/explicit-module-boundary-types": "off"
  }
}
